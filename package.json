{
  "name": "@sergiov85/skype-http",
  "description": "Unofficial Skype API for Node.js via HTTP. Forked from ocilo/skype-http and Sorunome/skype-http",
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/lib/index",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/SergioV85/skype-http.git"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --experimental-modules dist/example/main.js",
    "lint": "eslint \"./src/lib/**/*.ts\"",
    "lint:fix": "eslint \"./src/lib/**/*.ts\" --fix",
    "prepare": "npm run snyk-protect && npm run build",
    "prettier": "prettier --config .prettierrc --list-different \"./src/lib/**/*.{ts,json}\"",
    "prettier:autofix": "prettier --config .prettierrc --list-different \"./src/lib/**/*.{ts,json}\" --write",
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT='./src/test/tsconfig.spec.json' mocha --config ./.mocharc.js",
    "test:watch": "cross-env TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT='./src/test/tsconfig.spec.json' mocha --config ./.mocharc.js --watch",
    "test:coverage": "nyc npm run test",
    "start:old": "node --experimental-modules build/example/example/main.js",
    "prepare:old": "gulp all:tsconfig.json && gulp tslint.json && gulp dist",
    "prestart:old": "gulp example:build",
    "pretest:old": "npm run lint && gulp lib:build",
    "test:old": "gulp test",
    "test-online": "OCILO_TEST_ONLINE=true npm test",
    "prepublishOnly:old": "npm run test-online",
    "snyk-protect": "snyk protect"
  },
  "keywords": [
    "Skype",
    "Skype-Web",
    "Skype-HTTP"
  ],
  "dependencies": {
    "async-file": "^2.0.2",
    "big-integer": "^1.6.26",
    "bluebird": "^3.5.1",
    "cheerio": "^1.0.0-rc.3",
    "escape-html": "^1.0.3",
    "got": "^10.7.0",
    "incident": "^3.2.1",
    "js-sha256": "^0.9.0",
    "kryo": "^0.8.1",
    "lodash": "^4.17.20",
    "tough-cookie": "^4.0.0",
    "tunnel": "0.0.6",
    "snyk": "^1.437.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.1.7",
    "@types/cheerio": "^0.22.12",
    "@types/escape-html": "0.0.20",
    "@types/form-data": "^2.5.0",
    "@types/gulp": "^4.0.6",
    "@types/lodash": "^4.14.102",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.0.0",
    "@types/tough-cookie": "^2.3.2",
    "@types/tunnel": "0.0.1",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "del": "^3.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "husky": "^4.3.8",
    "minimist": "^1.2.5",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.0",
    "tslint": "^5.18.0",
    "turbo-gulp": "^0.22.1",
    "typescript": "^4.1.0"
  },
  "bugs": {
    "url": "https://github.com/SergioV85/skype-http/issues"
  },
  "homepage": "https://github.com/SergioV85/skype-http#readme",
  "author": "ocilo -> Sorunome -> SergioV85",
  "files": [
    "dist/lib/**/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier & npm run lint",
      "pre-push": "npm run test"
    }
  },
  "snyk": true
}
